<script>
  // Get the request boxes
  const requestBoxes = document.querySelectorAll('.request-box');

  // Get the pop-up form
  const popUpForm = document.getElementById('pop-up-form');

  // Get the black overlay
  const overlay = document.getElementById('overlay');

  // Add event listeners to the request boxes
  requestBoxes.forEach((box) => {
    box.addEventListener('click', () => {
      // Get the type of the request box
      const type = box.getAttribute('data-type');

      // Set the title and subject of the pop-up form
      const title = box.querySelector('h3').textContent;
      document.getElementById('pop-up-title').textContent = title;

      // Show the corresponding form
      const forms = popUpForm.querySelectorAll('form');
      forms.forEach((form) => {
        form.style.display = 'none';
      });
      const formId = `${type}-form`;
      const form = document.getElementById(formId);
      form.style.display = 'block';

      // Show the pop-up form and overlay
      popUpForm.style.display = 'block';
      overlay.style.display = 'block';

      // Fade in the pop-up form and overlay
      setTimeout(() => {
        popUpForm.style.opacity = 1;
        overlay.style.opacity = 1;
      }, 100);

      // Prevent scrolling on the body
      document.body.style.overflow = 'hidden';
    });
  });

  // Add event listener to the close button
  popUpForm.querySelector('.close-button').addEventListener('click', () => {
    // Fade out the pop-up form and overlay
    popUpForm.style.opacity = 0;
    overlay.style.opacity = 0;

    // Hide the pop-up form and overlay after fade out
    setTimeout(() => {
      popUpForm.style.display = 'none';
      overlay.style.display = 'none';
    }, 300);

    // Allow scrolling on the body
    document.body.style.overflow = 'auto';
  });

  // Add event listeners to the form submit buttons
  const forms = popUpForm.querySelectorAll('form');
  forms.forEach((form) => {
    form.addEventListener('submit', (e) => {
      e.preventDefault();

      // Get the form data
      const formData = new FormData(form);

      // Create a Discord webhook embed
      const embed = {
        title: form.id,
        description: '',
        fields: [],
        color: 0xff3939,
      };

      // Add form fields to the embed
      for (const [key, value] of formData) {
        if (value !== '') {
          embed.fields.push({
            name: key,
            value: value,
            inline: false,
          });
        }
      }

      // Check if the embed exceeds the character limit
      if (JSON.stringify(embed).length > 6000) {
        // Split the embed into multiple embeds
        const embeds = [];
        let currentEmbed = { ...embed };
        currentEmbed.fields = [];

        for (const field of embed.fields) {
          if (JSON.stringify(currentEmbed).length + JSON.stringify(field).length > 6000) {
            embeds.push(currentEmbed);
            currentEmbed = { ...embed };
            currentEmbed.fields = [field];
          } else {
            currentEmbed.fields.push(field);
          }
        }

        embeds.push(currentEmbed);

        // Send the embeds to the Discord webhook
        embeds.forEach((embed) => {
          fetch('https://discord.com/api/webhooks/1370610906572001330/zNHDI5-eu98mKi7oHTFVFr4fs3tWsLY94N38ra9_mxyC9DIHyGwfRrLOo8tPj3UWDUbx', {
            method: 'POST',
            headers: {
              'Content-Type': 'application/json',
            },
            body: JSON.stringify({
              embeds: [embed],
            }),
          });
        });
      } else {
        // Send the embed to the Discord webhook
        fetch('https://discord.com/api/webhooks/1370610906572001330/zNHDI5-eu98mKi7oHTFVFr4fs3tWsLY94N38ra9_mxyC9DIHyGwfRrLOo8tPj3UWDUbx', {
          method: 'POST',
          headers: {
            'Content-Type': 'application/json',
          },
          body: JSON.stringify({
            content: '<@&1357556658775130265>',
            embeds: [embed],
          }),
        });
      }

      // Fade out the pop-up form and overlay
      popUpForm.style.opacity = 0;
      overlay.style.opacity = 0;

      // Hide the pop-up form and overlay after fade out
      setTimeout(() => {
        popUpForm.style.display = 'none';
        overlay.style.display = 'none';
      }, 300);

      // Allow scrolling on the body
      document.body.style.overflow = 'auto';
    });
  });
</script>

